class Student {
	private int id;
	private String firstName,lastName;
	
	Student(int id, String firstName, String lastName) {
		this.id = id;
		this.firstName = firstName;
		this.lastName = lastName;
	}

	public String describe() {
		return id+" "+firstName+" "+lastName;
	}
}
